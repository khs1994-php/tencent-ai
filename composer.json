{
  "name": "khs1994/tencent-ai",
  "description": "Tencent AI SDK",
  "keywords": [
    "tencent",
    "ai",
    "sdk"
  ],
  "homepage": "https://github.com/khs1994-php/tencent-ai",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "khs1994",
      "email": "khs1994@khs1994.com",
      "homepage": "https://khs1994.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "khs1994/curl": "18.06.0",
    "khs1994/is": "18.06.0"
  },
  "require-dev": {
    "illuminate/console": "^5.5",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "TencentAI\\": "src/TencentAI/"
    },
    "files": [
      "src/TencentAI/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "TencentAI\\Tests\\": "tests/TencentAI/"
    }
  },
  "config": {
    "platform": {
      "php": "7.1.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "18.06-dev"
    },
    "laravel": {
      "providers": [
        "TencentAI\\ServiceProvider"
      ],
      "aliases": {
        "TencentAI": "TencentAI\\Facade"
      }
    }
  }
}
